<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <strong><%= f.label :title %><br></strong>
  <%= f.text_field :title %><br>
  <br />
  <strong><%= f.label :artist_name %><br></strong>
  <%= f.text_field :artist_name %><br>
  <br/>
  <strong><%= f.label :genre %><br></strong>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <br/>
  <strong><%= f.label "Notes"%><br></strong>

  <input name="song[notes][]" id="song_notes_1">
  <input name="song[notes][]" id="song_notes_2">
  <input name="song[notes][]" id="song_notes_3">

  <br>
  <br>
  <%= f.submit %>
<% end %>


